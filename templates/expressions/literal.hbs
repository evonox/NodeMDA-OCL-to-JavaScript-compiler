{{#switch literalType}}
    {{#case "boolean"}} {{> booleanLiteral }} {{/case}}
    {{#case "number"}} {{> numberLiteral }} {{/case}}
    {{#case "string"}} {{> string }} {{/case}}
    {{#case "tuple"}} {{> tuple }} {{/case}}
    {{#case "collection" }} {{> collection }} {{/case}}
    {{#case "enumeration"}} {{> enumerationLiteral }} {{/case}}
    {{#case "invalid"}} {{> invalidLiteral }} {{/case}}
    {{#case "null"}} {{> nullLiteral }} {{/case}}
{{/switch}}